{
  "name": "angular-2-d3-exercise",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm run typings install",
    "tsc": "tsc",
    "tsc:w": "tsc --watch",
    "tsc:exercise": "tsc --watch -p typescript-exercises",
    "live": "live-server --open=/",
    "start": "concurrently \"npm run tsc:w\" \"npm run live\" ",
    "exercise": "concurrently \"npm run tsc:exercise\" \"nodemon --ext ts -w typescript-exercises typescript-exercises/exercise.js\"",
    "typings": "typings"
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-beta.15",
    "d3": "^3.5.16",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.25",
    "zone.js": "0.6.10"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "live-server": "^0.9.2",
    "typescript": "^1.8.9",
    "typings": "^0.7.11"
  }
}
